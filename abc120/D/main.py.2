#!/usr/bin/env python3
import sys

def solve_iter(N: int, M: int, A: "List[int]", B: "List[int]"):
    ic = N * (N - 1) // 2
    uf = UnionFind(N+1)
    for a, b in zip(A[::-1], B[::-1]):
        yield ic
        if uf.same(a, b):
            continue
        ic -= uf.size(a) * uf.size(b)
        uf.union(a, b)

def solve(N: int, M: int, A: "List[int]", B: "List[int]"):
    for a in reversed(list(solve_iter(N, M, A, B))):
        print(a)

